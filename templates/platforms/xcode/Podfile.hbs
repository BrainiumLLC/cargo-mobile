# Uncomment the next line to define a global platform for your project
platform :ios, '{{apple.ios-min-version-major}}.{{apple.ios-min-version-minor}}'
platform :osx, '{{apple.osx-min-version-major}}.{{apple.osx-min-version-minor}}'

target '{{app.name}}_iOS' do
  # Pods for {{app.name}}_iOS
  {{~#each ios-pods}}
  pod '{{this}}'{{/each}}
end

target '{{app.name}}_macOS' do
  # Pods for {{app.name}}_macOS
  {{~#each macos-pods}}
  pod '{{this}}'{{/each}}
end
